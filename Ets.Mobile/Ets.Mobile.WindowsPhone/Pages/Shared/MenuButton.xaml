<HyperlinkButton
    x:Class="Ets.Mobile.Pages.Shared.MenuButton"
    xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
    xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
    xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
    xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
    mc:Ignorable="d"
    d:DesignHeight="60"
    d:DesignWidth="60">

    <Grid VerticalAlignment="Stretch" Height="18" Margin="0,-7,0,-10">
        <Grid.RowDefinitions>
            <RowDefinition Height="*"/>
            <RowDefinition Height="*"/>
            <RowDefinition Height="*"/>
        </Grid.RowDefinitions>
        <Line Grid.Row="0" VerticalAlignment="Top" StrokeEndLineCap="Round" StrokeThickness="2" Stroke="{StaticResource EtsRedMediumBrush}" X1="0" X2="25" Y1="0" Y2="0" />
        <Line Grid.Row="1" VerticalAlignment="Center" StrokeEndLineCap="Round" StrokeThickness="2" Stroke="{StaticResource EtsRedMediumBrush}" X1="0" X2="25" Y1="0" Y2="0" />
        <Line Grid.Row="2" VerticalAlignment="Bottom" StrokeEndLineCap="Round" StrokeThickness="2" Stroke="{StaticResource EtsRedMediumBrush}" X1="0" X2="25" Y1="0" Y2="0" />
    </Grid>
</HyperlinkButton>
