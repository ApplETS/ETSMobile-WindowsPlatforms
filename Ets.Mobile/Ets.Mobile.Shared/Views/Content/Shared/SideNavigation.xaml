<UserControl
    x:Class="Ets.Mobile.Views.Content.Shared.SideNavigation"
    xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
    xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
    xmlns:local="using:Ets.Mobile.Views.Content.Shared"
    xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
    xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
    xmlns:main="using:Ets.Mobile.Views.Content.Main"
    DataContext="{Binding}"
    mc:Ignorable="d"
    d:DesignHeight="800"
    d:DesignWidth="512">
    <Grid>
        <StackPanel>
            <StackPanel Orientation="Horizontal" Height="96">
                <Image Source="/Assets/Menu/Home-64.png" Width="25" Margin="25,0,0,0" />
                <TextBlock x:Uid="HomeUid" Margin="25,0,0,0" VerticalAlignment="Center" Text="Home" Style="{StaticResource EtsTextBlockWhite4}" />
            </StackPanel>
            <StackPanel Orientation="Horizontal" Height="45">
                <Image Source="/Assets/Menu/Home-64.png" Width="25" Margin="25,0,0,0" />
                <TextBlock x:Uid="HomeUid" Margin="25,0,0,0" VerticalAlignment="Center" Text="Home" Style="{StaticResource EtsTextBlockWhite4}" />
            </StackPanel>
        </StackPanel>
        
    </Grid>
</UserControl>
